{
  "manifest_version": 3,
  "name": "Souip",
  "version": "1.0",
  "description": "Une extension pour afficher les favoris de l'utilisateur (so far).",
  "permissions": [
    "bookmarks"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}